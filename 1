#начало программы
#иницализируем массив
#перебираем правую границу
#перебираем элементы до текущей правой границы
#если текущей элемент больше следующего то поменять их местами
#конец программы

import random
n = 10
l = 0
r = 100
a = [random.randint(l, r) for i in range(n)]
for i in range(n):
    flag = False
    for j in range(n-i-1):
        if (a[j] > a[j+1]):
            flag = True
            a[j], a[j+1] = a[j+1], a[j]
    if (not flag):
        break
print(*a)
